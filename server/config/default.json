{
  "app": {
    "port": 5000,
    "env": "development"
  },
  "rateLimit": {
    "windowMs": 60000,
    "max": 100
  },
  "rService": {
    "url": "http://localhost:8000",
    "processEndpoint": "/clean?jobId="
  },
  "redis": {
    "url": "redis://redis:6379"
  },
  "database": {
    "url": "postgresql://postgres:postgres@postgres:5432/qualimind?schema=public"
  },
  "security": {
    "jwtSecret": "dev-secret-change-me",
    "jwtExpiresIn": "1h"
  },
  "googleAuth": {
    "clientId": "YOUR_GOOGLE_CLIENT_ID",
    "clientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "redirectUri": "https://your-api-domain.com/api/auth/google/callback"
  },
  "multer": {
    "dest": "./raw/uploads"
  }
}

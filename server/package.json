{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=development APP_ENV=development nodemon --watch src --ext ts --exec \"node -r ts-node/register -r tsconfig-paths/register src/index.ts\"",
    "test:run": "cross-env NODE_ENV=testing APP_ENV=testing vitest run --require tsconfig-paths/register",
    "start:predeploy": "cross-env NODE_ENV=predeploy APP_ENV=predeploy node dist/src/index.js",
    "start:prod": "cross-env NODE_ENV=production APP_ENV=production node dist/src/index.js",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "prisma:generate": "node scripts/run-prisma.js generate",
    "prisma:migrate": "node scripts/run-prisma.js migrate dev",
    "prisma:studio": "node scripts/run-prisma.js studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bullmq": "^5.62.2",
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "dotenv-flow": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.8.2",
    "morgan": "^1.10.1",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  }
}
